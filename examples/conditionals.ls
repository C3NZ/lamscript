var x = true;

// Your typical if/else statement.
if (x) {
    print "x is true";
} else {
    print "x is false";
}


var num = 10;

// If/else if statements can be chained together too!
if (num < 5) {
    print "num is less than 5";
} else if (num < 8) {
    print "num is less than 8";
} else if (num < 11) {
    print "num is less than 11";
}
